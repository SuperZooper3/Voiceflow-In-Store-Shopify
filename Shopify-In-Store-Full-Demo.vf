      "createdByID": 889698,
      "folderID": null,
      "code": "export default async function main(args) {\n  // Destructuring to get orderId from inputVars\n  const { email } = args.inputVars;\n\n  // Check if orderId is provided\n  if (!email) {\n    return {\n      next: { path: 'error' },\n      trace: [{\n        type: 'text',\n        payload: { message: \"Missing email\" }\n      }]\n    };\n  }\n  \n  //Paste the endpoint we gave you inside our app here\n  const requestUrl = `paste here`;\n\n  try {\n    // Making the GET request to the specified endpoint\n    const response = await fetch(requestUrl, {\n      method: 'GET',\n      headers: {\n         \"Authorization\": \"Bearer gsk-xGZjqxkDrmeYjdYWGQmpBCcZhFzzNfh2\"\n  }\n    });\n\n    if (!response.ok) {\n      throw new Error(`HTTP error! status: ${response.status}`);\n    }\n\n    const orderData = response.json; \n\n    // Ensure the response contains order data\n    if (!orderData) {\n      throw new Error(`Order not found or missing data. ${responseString}`);\n    }\n\n\n  // Build carousel object from the order data\n  const carousel = {\n    'layout': 'Carousel',\n    'cards': []\n  };\n\n  // Iterate over each order in the data array\n  orderData.data.forEach((order) => {\n    const title = order.name;\n    const body = `Status: On its way`;\n    const statusUrl = order.order_status_url\n    const image = order.product_image\n\n    carousel.cards.push({\n      imageUrl: image, // Replace with a relevant image URL if available\n      title: title,\n      description: {\n        text: body\n      },\n      buttons: [\n        {\n          name: \"View Order\",\n          payload: {\n            actions: [\n              {\n                type: \"open_url\",\n                url: statusUrl // Replace with a URL to view the order if available\n              }\n            ]\n          }\n        }\n      ]\n    });\n  });\n\n  // Return the carousel trace\n  return {\n    next: { path: 'success' },\n    trace: [\n      {\n        type: \"carousel\",\n        payload: carousel\n      }\n    ]\n  };\n\n  } catch (error) {\n    return {\n      next: { path: 'error' },\n      trace: [{\n        type: 'debug',\n        payload: { message: `Failed to fetch order data: ${error.message}` }\n      }]\n    };\n  }\n\n\n\n/*\n    const order = data;\n    \n\n    return {\n      outputVars: { orderName: order.name, totalPrice: order.total_price, orderStatusUrl: order.order_status_url },\n      next: { path: 'success' },\n      trace: [\n        { type: 'debug', payload: { message: `order found` } }\n      ]\n    };\n\n*/\n\n}",
